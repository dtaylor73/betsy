<section class="merchant-details">
  <p class="merchant-title">ALL MERCHANTS</p>
  <table class='table table-borderless'>
    <% @merchants.each do |merchant| %>
      <tr>
        <td> <%= link_to merchant.username, merchant_products_path(merchant) %> </td>
      </tr>
    <% end %>
  </table>
</section>